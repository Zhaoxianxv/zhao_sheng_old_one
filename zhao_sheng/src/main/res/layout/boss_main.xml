<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/WhiteSmoke">

    <com.yfy.view.SQToolBar style="@style/App_SQToolbar"/>


    <RelativeLayout
        android:id="@+id/base_layout_one"
        android:background="@color/White"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/margin_10dp"
        android:layout_height="@dimen/margin_70dp">
        <android.support.v7.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_keyboard_arrow_right_gray_24dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/margin_10dp"
            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="教师请假"
                android:layout_marginStart="@dimen/margin_10dp"
                android:textSize="@dimen/textsize10"
                android:textColor="@color/app_base_color"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="教师员工请假信息"
                android:layout_marginStart="@dimen/margin_10dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:textSize="@dimen/textsize6"
                android:textColor="@color/Gray"/>
        </LinearLayout>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/base_layout_two"
        android:background="@color/White"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/margin_10dp"
        android:layout_height="@dimen/margin_70dp">
        <android.support.v7.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_keyboard_arrow_right_gray_24dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/margin_10dp"
            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="后勤保修"
                android:layout_marginStart="@dimen/margin_10dp"
                android:textSize="@dimen/textsize10"
                android:textColor="@color/app_base_color"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="教师员保修信息"
                android:layout_marginStart="@dimen/margin_10dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:textSize="@dimen/textsize6"
                android:textColor="@color/Gray"/>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/base_layout_three"
        android:background="@color/White"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/margin_10dp"
        android:layout_height="@dimen/margin_70dp">
        <android.support.v7.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_keyboard_arrow_right_gray_24dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/margin_10dp"
            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"

            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="功能室"
                android:layout_marginStart="@dimen/margin_10dp"
                android:textSize="@dimen/textsize10"
                android:textColor="@color/app_base_color"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="校园多功能教室使用申请信息"
                android:layout_marginStart="@dimen/margin_10dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:textSize="@dimen/textsize6"
                android:textColor="@color/Gray"/>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/base_layout_four"
        android:background="@color/White"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_marginTop="@dimen/margin_10dp"
        android:layout_height="@dimen/margin_70dp">
        <android.support.v7.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_keyboard_arrow_right_gray_24dp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/margin_10dp"
            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"

            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="考勤"
                android:layout_marginStart="@dimen/margin_10dp"
                android:textSize="@dimen/textsize10"
                android:textColor="@color/app_base_color"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="教师员工考勤打卡信息"
                android:layout_marginStart="@dimen/margin_10dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:textSize="@dimen/textsize6"
                android:textColor="@color/Gray"/>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
