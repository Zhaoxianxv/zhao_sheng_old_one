<resources>


    <style name="AppThemeCompat" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/app_base_color</item>
        <item name="colorPrimaryDark">@color/app_base_color</item>
        <item name="colorAccent">@color/app_base_color</item>
        <item name="android:textColor">@color/app_base_text_color</item>
        <item name="actionMenuTextColor">@color/WhiteSmoke</item>
        <item name="android:windowBackground">@color/app_base_window_bg_color</item>
        <!-- Customize your theme here. -->

    </style>

    <style name="AppThemeWindow" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/app_base_color</item>
        <item name="colorPrimaryDark">@color/app_base_color</item>
        <item name="colorAccent">@color/app_base_color</item>
        <item name="android:textColor">@color/app_base_text_color</item>
        <item name="actionMenuTextColor">@color/White</item>
        <item name="android:windowBackground">@color/app_base_color</item>
        <!-- Customize your theme here. -->
    </style>

    <!--没有actionbar background-->
    <!--android:windowIsTranslucent=true  screenOrientation="portrait" 不共存-->
    <style name="AppThemeInitial" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/app_base_color</item>
        <item name="colorPrimaryDark">@color/public_transparent</item>
        <item name="colorAccent">@color/app_base_color</item>
        <item name="android:textColor">@color/app_base_text_color</item>
        <item name="android:windowBackground">@color/colorPrimary</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="AppThemeDialog" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>


    <style name="AppThemeNoDark" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/app_base_color</item>
        <item name="colorPrimaryDark">@color/app_base_color</item>
        <item name="colorAccent">@color/app_base_color</item>
        <item name="android:textColor">@color/app_base_text_color</item>
        <item name="android:windowBackground">@color/app_base_window_bg_color</item>
        <!-- Customize your theme here. -->

    </style>


    <style name="AppBaseTheme" parent="AppThemeCompat">

    </style>

    <style name="AppTheme" parent="AppThemeCompat">
        <item name="actionMenuTextColor">@color/WhiteSmoke</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>


    <style name="activity_change_theme" parent="AppThemeCompat">
        <item name="android:windowAnimationStyle">@style/ActivityAnimation</item>
    </style>
    <!-- Base application theme. -->

    <style name="AppThemeCompat.Screen">
        <item name="android:windowNoTitle">true</item>
    </style>


    <style name="App_SQToolbar">
        <item name="android:id">@id/sq_toolbar</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_45dp</item>
        <item name="android:background">@color/main_red</item>
    </style>


    <style name="checkbox_null">
        <item name="android:layout_width">@dimen/margin_25dp</item>
        <item name="android:layout_height">@dimen/margin_25dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:contentDescription">@string/app_name</item>

    </style>


    <style name="bottom_menu_linel">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="bottom_menu_iv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="ActivityAnimation">
        <item name="android:activityOpenEnterAnimation">@anim/activity_close_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/activity_close_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/activity_open_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/activity_open_exit</item>
    </style>

    <!-- 招生提示动画 -->
    <style name="dialogWindowAnim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/scale_point</item>
    </style>

    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="CustomProgressDialog" parent="@style/CustomDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <style name="Dialog_menu" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="anim_popup_dir">
        <item name="android:windowEnterAnimation">@anim/slide_in</item>
        <item name="android:windowExitAnimation">@anim/slide_out</item>
    </style>

    <style name="contacts_group">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/contact_group_height</item>
        <item name="android:drawablePadding">15dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">@dimen/textsize10</item>
        <item name="com.example.zhao_sheng:lHeight">30dp</item>
        <item name="com.example.zhao_sheng:lWidth">30dp</item>
        <item name="com.example.zhao_sheng:rHeight">15dp</item>
        <item name="com.example.zhao_sheng:rWidth">15dp</item>
    </style>

    <style name="dialogWindowAnim2" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit_anim</item>
    </style>

    <style name="leave_popup_anim_style">
        <item name="android:windowEnterAnimation">@anim/popup_for_leave_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_for_leave_exit</item>
    </style>

    <style name="BottomItem">
        <item name="android:layout_width">@dimen/margin_100dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginLeft">@dimen/margin_0dp</item>
        <item name="android:layout_marginRight">@dimen/margin_0dp</item>
        <item name="android:background">@color/White</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="BottomIcon_gone">
        <item name="android:layout_width">@dimen/margin_0dp</item>
        <item name="android:layout_height">@dimen/margin_0dp</item>
    </style>

    <style name="dateStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">16sp</item>
    </style>
    <!-- 长按钮-->
    <style name="long_btn">
        <item name="android:layout_width">@dimen/margin_150dp</item>
        <item name="android:layout_height">@dimen/margin_45dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <!--checkbox style-->
    <style name="CheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_selector</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <!--RadioButtom style-->
    <style name="RadioButtomAdd" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:button">@drawable/radiobutton_add_selector</item>
    </style>

    <style name="RadioButtomMinus" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:button">@drawable/radiobutton_minus_selector</item>
    </style>


    <!--toolbar style-->
    <style name="ToolBarStyle" parent="Theme.AppCompat">
        <item name="android:actionBarSize">@dimen/margin_35dp</item>
        <item name="actionMenuTextColor">@android:color/white</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>

        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="CommonLoadingProgressBar">
        <item name="android:layout_width">@dimen/margin_15dp</item>
        <item name="android:layout_height">@dimen/margin_15dp</item>
        <item name="android:layout_marginLeft">@dimen/margin_10dp</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:indeterminateDrawable">@drawable/progress_loading</item>
        <item name="android:indeterminateDuration">700</item>
        <item name="android:indeterminateOnly">true</item>
    </style>

    <!-- 仿 iOS 底部弹出动画 -->
    <!--<style name="BottomDialogAnim" parent="android:Animation">-->
    <!--<item name="@android:windowEnterAnimation">@anim/dialog_enter</item>-->
    <!--<item name="@android:windowExitAnimation">@anim/dialog_exit</item>-->
    <!--</style>-->

    <style name="KLineTabTextAppearance" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">15dp</item>
    </style>
</resources>