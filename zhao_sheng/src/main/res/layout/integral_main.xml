<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:orientation="vertical"    android:background="@color/AliceBlue"    android:layout_height="match_parent">    <com.yfy.view.SQToolBar style="@style/App_SQToolbar"/>    <View        android:layout_width="match_parent"        android:layout_height="0.5dp"        android:layout_alignParentBottom="true"        android:layout_marginBottom="@dimen/margin_45dp"        android:background="@color/a3_gray"/>    <TextView        android:id="@+id/integral_user_out"        android:layout_width="match_parent"        android:layout_height="@dimen/margin_45dp"        android:gravity="center"        android:layout_alignParentBottom="true"        android:text="@string/out_login"/>    <ListView        android:id="@+id/integral_main_list"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginTop="@dimen/margin_45dp"        android:layout_marginBottom="@dimen/margin_50dp"        android:scrollbars="none"/></RelativeLayout>